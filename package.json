{
  "name": "configs-og",
  "version": "1.0.0",
  "description": "Eslint, Prettier, Babel, Husky, lint-staged configurations for JavaScript Frontend environment",
  "keywords": [
    "eslint",
    "prettier",
    "babel",
    "husky",
    "lint-staged",
    "og",
    "oleggrishechkin"
  ],
  "homepage": "https://github.com/oleggrishechkin/configs-og",
  "license": "MIT",
  "author": {
    "name": "Oleg Grishechkin",
    "email": "oleggrishechkin@gmail.com",
    "url": "https://github.com/oleggrishechkin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oleggrishechkin/configs-og"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "dependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-function-bind": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.10.1",
    "eslint-plugin-import": "~2.19.1",
    "eslint-plugin-prettier": "~3.1.4",
    "eslint-plugin-react": "~7.19.0",
    "eslint-plugin-react-hooks": "~2.5.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "~1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "eslint --quiet"
    ]
  }
}
